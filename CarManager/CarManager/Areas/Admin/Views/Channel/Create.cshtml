@model CarManager.Areas.Admin.Models.ChannelModel
@{
    ViewBag.Title = Resource.Channel;
}

<div class="page-title">
    <div class="title_left">
        <h3>
            @Resource.Channel
            <small>@Resource.Create</small>
        </h3>
    </div>
</div>

<div class="main-content">
    @using (Html.BeginForm("Create", "Channel", FormMethod.Post))
    {
        @Html.AntiForgeryToken()

        <div class="form-horizontal">
            @Html.ValidationSummary(true)
            @Html.EditorForModel()

            <div class="form-group">
                <div class="col-md-offset-2 col-sm-offset-3 col-md-10 col-sm-9 col-xs-12">
                    <input type="submit" value="@Resource.Save" name="SaveMode" class="btn btn-primary" />
                    <input type="submit" value="@Resource.SaveAndContinueEdit" name="SaveMode" class="btn btn-primary" />
                    <a href="@Url.Action("Index")" class="btn btn-default">@Resource.BackToList</a>
                </div>
            </div>
        </div>
    }
</div>

@section scripts{
    <script>

    $(document).ready(function () {
        $('select').select2({
            theme: "bootstrap"
        });
    });

    </script>
}